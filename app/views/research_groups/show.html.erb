                    <div class="controls">
                        <%= @research_group.laboratories %>
                    </div>
                </div>
                            <div class="control-group span12 no-margin-left">
                    <label class="control-label">Status:</label>
                    <div class="controls">
                        <%= @research_group.status %>
                    </div>
                </div>
                    </div>
    </div>
</div>
